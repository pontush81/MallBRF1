{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/pages/visible",
      "dest": "server/server.js",
      "methods": ["GET"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,HEAD,OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type,Authorization,Origin,Accept,X-Requested-With,x-vercel-protection-bypass",
        "x-vercel-protection-bypass": "true",
        "x-vercel-skip-auth": "true"
      }
    },
    {
      "src": "/api/pages/published",
      "dest": "server/server.js",
      "methods": ["GET"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,HEAD,OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type,Authorization,Origin,Accept,X-Requested-With,x-vercel-protection-bypass",
        "x-vercel-protection-bypass": "true",
        "x-vercel-skip-auth": "true"
      }
    },
    {
      "src": "/api/pages/slug/(.*)",
      "dest": "server/server.js",
      "methods": ["GET"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,HEAD,OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type,Authorization,Origin,Accept,X-Requested-With,x-vercel-protection-bypass",
        "x-vercel-protection-bypass": "true",
        "x-vercel-skip-auth": "true"
      }
    },
    {
      "src": "/manifest.json",
      "dest": "build/manifest.json",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Content-Type": "application/json",
        "x-vercel-skip-auth": "true"
      }
    },
    {
      "src": "/favicon.ico",
      "dest": "build/favicon.ico",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "x-vercel-skip-auth": "true"
      }
    },
    {
      "src": "/(.*)",
      "dest": "server/server.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,HEAD,PUT,PATCH,POST,DELETE,OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type,Authorization,Origin,Accept,X-Requested-With,x-vercel-protection-bypass",
        "x-vercel-protection-bypass": "true"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "alias": ["stage.gulmaran.com"]
} 